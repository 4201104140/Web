@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <title>Excel Add-In with Commands Sample</title>

    <!-- To enable offline debugging using a local reference to Office.js, use:  -->
    <!-- <script src="Scripts/Office/MicrosoftAjax.js" type="text/javascript"></script>  -->
    <!-- <script src="Scripts/Office/1/office.js" type="text/javascript"></script>  -->
    <script src="https://appsforoffice.microsoft.com/lib/1/hosted/office.js" type="text/javascript"></script>
    <link href="~/UI/semantic.min.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/UI/semantic.min.js"></script>
    <link href="~/Content/excel.css" rel="stylesheet" />

</head>
<body>
    <div class="ui menu">
        <select class="ui search dropdown">
            <option value="">Chọn</option>
            <option value="AL">Sản phẩm</option>
            <option value="AK">Thống kê</option>
            <option value="AZ">Khách hàng</option>
        </select>
        <div class="right menu">
            <button class="ui olive button" id="exportExcel">Xuất</button>
        </div>
    </div>
    <div class="ui equal width form">
        <div class="fields">
            <div class="field">
                <label>Từ ngày</label>
                <input type="text" placeholder="Username">
            </div>
            <div class="field">
                <label>Đến ngày</label>
                <input type="text" placeholder="Username">
            </div>
        </div>
        <div class="fields">
            <div class="field">
                <label>First name</label>
                <input type="text" placeholder="First Name">
            </div>
            <div class="field">
                <label>First name</label>
                @*<div class="ui input left icon">
                        <i class="calendar icon"></i>
                        <input type="text" placeholder="Date/Time">
                    </div>*@
            </div>
            <div class="field">
                <label>Last name</label>
                <input type="text" placeholder="Last Name">
            </div>
        </div>
    </div>
    <script src="~/Scripts/getdata.js"></script>
    <script src="~/Scripts/excel.js"></script>
</body>
</html>
